            <script>
                var feedback_count = 0;
                
                $('#p_submit').click(function(e) {
                    e.preventDefault();
                    $.ajax({
                        global: false,
                        type: 'POST',
                        url: '/feedback/product/submit', // missing quotes  
                        dataType: 'html',
                        data: {
                            'p_rating': document.querySelector('input[name="p_rating"]:checked').value,
                            'p_comment': $("#p_comment").val(), 
                            'order_id': "<%=orderid%>"
                        },
                        success: function (result) {    
                            $('.p_card_body').css("display","none");
                            $('.p-thank-you').css("display","block");
                            feedback_count+=1;
                           if(feedback_count == 2){
                                $('.gobtn').css('display','block');
                            } 

                        },
                        error: function (request, status, error) {
                            serviceError();
                        }
                    });
                });
                $('#s_submit').click(function(e) {
                    e.preventDefault();
                    $.ajax({
                        global: false,
                        type: 'POST',
                        url: '/feedback/seller/submit', // missing quotes  
                        dataType: 'html',
                        data: {
                            's_rating': document.querySelector('input[name="s_rating"]:checked').value,
                            's_comment': $("#s_comment").val(), 
                            'order_id': "<%=orderid%>"
                        },
                        success: function (result) {    
                            $('.s_card_body').css("display","none");
                            $('.s-thank-you').css("display","block");
                            feedback_count+=1;
                            if(feedback_count == 2){
                                $('.container').css({'display':'block','text-align':'center'});

                            }   

                        },
                        error: function (request, status, error) {
                            serviceError();
                        }
                    });
                });
            </script>
