<div class="row">
    <div class="col col-md-6 col-sm-12">
        <div class="card feedback-card">
                <h5 class="card-header">Product Feedback</h5>
                <div class="card-body p_card_body">
                    <img src="" alt="product-img">
                    <form action="" method="">
                    <fieldset class="rating p_rating" id="p_rating_fieldset">
                        <input type="radio" id="p_star5" name="p_rating" value="5" />
                        <label class = "p_full" for="p_star5" title="Awesome - 5 stars"></label>
                        <input type="radio" id="p_star4half" name="p_rating" value="4.5" />
                        <label class="p_half" for="p_star4half" title="Pretty good - 4.5 stars"></label>
                        <input type="radio" id="p_star4" name="p_rating" value="4" />
                        <label class = "p_full" for="p_star4" title="Pretty good - 4 stars"></label>
                        <input type="radio" id="p_star3half" name="p_rating" value="3.5" />
                        <label class="p_half" for="p_star3half" title="Meh - 3.5 stars"></label>
                        <input type="radio" id="p_star3" name="p_rating" value="3" />
                        <label class = "p_full" for="p_star3" title="Meh - 3 stars"></label>
                        <input type="radio" id="p_star2half" name="p_rating" value="2.5" />
                        <label class="p_half" for="p_star2half" title="Kinda bad - 2.5 stars"></label>
                        <input type="radio" id="p_star2" name="p_rating" value="2" />
                        <label class = "p_full" for="p_star2" title="Kinda bad - 2 stars"></label>
                        <input type="radio" id="p_star1half" name="p_rating" value="1.5" />
                        <label class="p_half" for="p_star1half" title="Meh - 1.5 stars"></label>
                        <input type="radio" id="p_star1" name="p_rating" value="1" />
                        <label class = "p_full" for="p_star1" title="Sucks big time - 1 star"></label>
                        <input type="radio" id="p_starhalf" name="p_rating" value="0.5" />
                        <label class="p_half" for="p_starhalf" title="Sucks big time - 0.5 stars"></label>
                    </fieldset>
                    <h5>Seller Name</h5>    
                    
                <h5 class="card-title">Write a review</h5>
                <textarea id="p_comment" name="product_review" cols="50" rows="5" maxlength="256"></textarea>
              <button id="p_submit" class="btn btn-primary submit-feedback">Submit</button>
            </form>
            </div>
            <div class="card-body p-thank-you" style="display: none;">
